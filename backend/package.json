    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.4.20",
    "@supabase/supabase-js": "^2.38.0",
    "@types/multer": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "langchain": "^0.3.35",
    "multer": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/express": "^4.17.17",
